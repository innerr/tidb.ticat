help = run workload and record the result to meta db
abbr = bench.run.work

flow = mark-time bench.run.begin : [[bench.workload]].run : mark-time bench.run.end

[arg]
workload|wl = ''

[val2env]
bench.run.begin = n/a
bench.run.end = n/a
bench.run.log = n/a
bench.run.score = -1

[arg2env]
bench.workload = workload

[env]
bench.workload = read
bench.run.begin = write
bench.run.end = write
